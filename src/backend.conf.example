[main]
# main domain
domain=p.sofatutor-sandbox.de

# default ttl
ttl=432000

# default IP address for non-wildcard entries
ipaddress=3.71.243.88

# SOA
[soa]
# serial number
id=1
# Hostmaster email address
hostmaster=hostmaster@p.sofatutor-sandbox.de
# Name server
ns=ns1.p.sofatutor-sandbox.de
refresh=10800
retry=3600
expire=604800
minimum=3600

# nameservers
[nameservers]
ns1.p.sofatutor-sandbox.de=3.71.243.88
ns2.p.sofatutor-sandbox.de=3.71.243.88

[additional_cnames]
_acme-challenge=xyz.auth.p.sofatutor-sandbox.de.
www=www.p.sofatutor-sandbox.de

[whitelist]
loopback = 127.0.0.0/8
private_net_10 = 10.0.0.0/8
private_net_172_16 = 172.16.0.0/12
private_net_192_168 = 192.168.0.0/16
